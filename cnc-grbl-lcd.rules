
(rules PCB cnc-grbl-lcd
  (snap_angle 
    fortyfive_degree
  )
  (autoroute_settings
    (fanout off)
    (autoroute on)
    (postroute on)
    (vias on)
    (via_costs 50)
    (plane_via_costs 5)
    (start_ripup_costs 100)
    (start_pass_no 150)
    (layer_rule F.Cu
      (active on)
      (preferred_direction horizontal)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 2.4)
    )
    (layer_rule B.Cu
      (active on)
      (preferred_direction vertical)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 1.7)
    )
  )
  (rule
    (width 300.0)
    (clear 200.2)
    (clear 150.0 (type smd_to_turn_gap))
    (clear 300.2 (type default_Power))
    (clear 50.0 (type smd_smd))
    (clear 300.2 (type smd_Power))
    (clear 300.2 (type "kicad_default"_Power))
    (clear 300.2 (type Power_Power))
  )
  (padstack "Via[0-1]_600:400_um"
    (shape
      (circle F.Cu 600.0 0.0 0.0)
    )
    (shape
      (circle B.Cu 600.0 0.0 0.0)
    )
    (attach off)
  )
  (padstack "Via[0-1]_1200:400_um"
    (shape
      (circle F.Cu 1200.0 0.0 0.0)
    )
    (shape
      (circle B.Cu 1200.0 0.0 0.0)
    )
    (attach off)
  )
  (via 
    "Via[0-1]_600:400_um" "Via[0-1]_600:400_um" default
  )
  (via 
    "Via[0-1]_1200:400_um" "Via[0-1]_1200:400_um" default
  )
  (via 
    "Via[0-1]_600:400_um-kicad_default" "Via[0-1]_600:400_um" "kicad_default"
  )
  (via 
    "Via[0-1]_1200:400_um-kicad_default" "Via[0-1]_1200:400_um" "kicad_default"
  )
  (via 
    "Via[0-1]_600:400_um-Power" "Via[0-1]_600:400_um" Power
  )
  (via 
    "Via[0-1]_1200:400_um-Power" "Via[0-1]_1200:400_um" Power
  )
  (via_rule
    default "Via[0-1]_600:400_um"
  )
  (via_rule
    "kicad_default" "Via[0-1]_600:400_um-kicad_default"
  )
  (via_rule
    Power "Via[0-1]_1200:400_um-Power"
  )
  (class default
    (clearance_class default)
    (via_rule default)
    (rule
      (width 300.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class "kicad_default"
    GND reset +5V "TX_FTDI" "RX_FTDI" RX TX "reset-ftdi"
    RST HLD STR
    (clearance_class "kicad_default")
    (via_rule kicad_default)
    (rule
      (width 300.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class Power
    (clearance_class Power)
    (via_rule Power)
    (rule
      (width 800.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
)